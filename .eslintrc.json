{
    "extends": [
    	"airbnb",
    	"prettier"
    ],

    "plugins": [
    	"prettier"
    ],

    "parserOptions": {
    	"ecmaVersion": 2016,
    	"sourceType": "module",
    	"ecmaFeatures": {
    		"jsx": true
    	}
    },

    "env": {
    	"browser": true,
    	"node": true,
    	"es6": true
    }
}